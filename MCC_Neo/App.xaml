        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Recursos/EstilosExtras.xaml"/>
                <ResourceDictionary Source="/Recursos/TabControlStyle.xaml"/>
